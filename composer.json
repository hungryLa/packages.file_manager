{
    "name": "niyama/file-manager",
    "autoload": {
        "psr-4": {
            "Niyama\\FileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niyama\\FileManager\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Niyaz",
            "email": "hungry_la@mail.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "intervention/image": "^3.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niyama\\FileManager\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Niyama\\FileManager\\Facades"
            }
        }
    },
    "scripts": {
        "test" : [
            "vendor/bin/phpunit"
        ]
    }
}
